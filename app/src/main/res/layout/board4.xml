    <TextView
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/textView1"
        android:background="@color/bg"
        android:textStyle="bold"
        android:textColor="?attr/colorControlNormal"
        android:textSize="40dp"
        android:padding="5dp"/>

    <LinearLayout
